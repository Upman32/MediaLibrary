{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended"
  ],
  "parserOptions": {
    "ecmaFeatures": {
        "jsx": true
    },
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": [
    "react"
  ],
  "rules": {
    "quotes":["error", "single"],
    "semi":["error", "never"],
    "indent":["error", 2],
    "no-multi-spaces":["error"],
    "no-multiple-empty-lines":[2, {"max":1}],
    "react/react-in-jsx-scope":["off"]
  }
}
